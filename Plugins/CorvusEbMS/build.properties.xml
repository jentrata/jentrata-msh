<?xml version="1.0" encoding="UTF-8"?>

<properties>
	<build>
		<dir location="../build" />
	</build>
	<dist>
		<dir location="../dist" />
		<jar name="corvus-ebms-core" />
		<files>*.spa,lib/*.jar</files>
	</dist>
	<project>
		<dir location="../"/>
		<base location="../../"/>
		<class>
			<lib location="../lib" />
			<ext location="../../" files="Commons/ext/*.jar,Commons/dist/lib/*.jar,Corvus/dist/lib/*.jar,CorvusEbMS/dist/lib/*.jar"/> 
			<!-- <import location="../../ebxml-pkg/dist/lib" /> -->
			<import location="" />
			<path></path>
		</class>
		<source>
			<path>../src</path>
		</source>
		<spa dir="../spa">
			<plugin name="hk.hku.cecid.ebms" />
		</spa>
		<test>
			<path location="../test" />
		</test>
	</project>
	<deploy target="corvus">
		<corvus dir="C:/corvus/plugins"/>
	</deploy>
</properties>
